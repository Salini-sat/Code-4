#include<stdio.h>
int main(){
    int arr[100][100];
    int n;
    int count =0;
    int sum =0;
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            
            

        }
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(i==j){
                sum = sum + arr[i][j];
            }
            else if(i+j==(n-1)){
                count = count + arr[i][j];
            }
        }
    }
    int diff = sum - count;
    printf("%d",diff);
   
}
